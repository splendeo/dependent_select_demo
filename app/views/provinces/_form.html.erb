<% form_for @province do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :country %><br />
    <%= f.collection_select :country_id, @countries, :id, :name, :include_blank => true %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
